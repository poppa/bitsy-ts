  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "program": "${workspaceFolder}/src/mod.ts",
      "request": "launch",
      "type": "node",
      "sourceMaps": true,
      "env": {
        "NODE_ENV": "development",
        "TEST_TOKENIZER": "1"
      },
      "outputCapture": "console",
      "smartStep": true,
      "console": "integratedTerminal",
      "preLaunchTask": "tsc: build - tsconfig.json"
    }
  ]
}
